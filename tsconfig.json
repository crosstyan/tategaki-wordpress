{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
        "baseUrl": ".",
        "paths": {
					// name and path to aliases
					// https://codingcat.dev/post/make-pathing-easier-with-aliases-in-sveltekit
					"@/*": ["src/*"],
					"@": ["src"],
					"$lib":["src/lib"],
					"$lib/*":["src/lib/*"]
        }
    },
}
